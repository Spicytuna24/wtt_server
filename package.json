    "fs": "^0.0.1-security",
    "http": "^0.0.0",
    "https": "^1.0.0",
    "mapbox-gl": "^1.10.1",
    "moment": "^2.24.0",
    "morgan": "^1.9.1",
    "node-fetch": "^1.7.3",
    "nodemon": "^2.0.4",
    "pg": "^8.1.0",
    "pg-promise": "^10.6.1",
    "pg-native": "^3.0.0",
    "pgtools": "^0.3.0",
    "validator": "^11.1.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
